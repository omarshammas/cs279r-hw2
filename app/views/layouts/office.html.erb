<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	 <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
	<title>Command Selection Experiment</title>
	<link href="assets/ribbon/ribbon.css" rel="stylesheet" type="text/css" />
	<link href="assets/cm/commandmaps.css" rel="stylesheet" type="text/css" />
	<link href="assets/ribbon/soft_button.css" rel="stylesheet" type="text/css" />
	<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js"></script>
	<script src="http://cdn.jquerytools.org/1.2.7/full/jquery.tools.min.js"></script>
	<script type="text/javascript" src="assets/ribbon/ribbon.js"></script>
	<script type="text/javascript">
		$(document).ready(function () {
			$('#ribbon').ribbon();
					
			
			if (document.addEventListener)
    		{
       			document.addEventListener("keydown",keydown,false);
     		}
     		else if (document.attachEvent){
     			document.attachEvent("onkeydown"
					,keydown)
					}
					else {
						document.onkeydown = keydown
					}
					function keydown(e) {
						if (!e)
							e = event;
						if (!e.ctrlKey)
							return;
						if ($("#commandmap").overlay().isOpened()) {
							$("#commandmap").overlay().close();
						} else {
							$("#commandmap").overlay().load();
						}
					}
					});
	</script>

</head>
<body bgcolor="#c9cdd2">
	
	<%= yield %>

</body>
</html>